{
  "name": "showtimes",
  "description": "show movie showtimes",
  "version": "1.0.0",
  "author": "Chen Wilson",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon server.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=development webpack -d --watch --progress --hide-modules",
    "test": "jest",
    "seed-all": "node_modules/.bin/sequelize db:seed:all"
  },
  "dependencies": {
    "bootstrap": "^4.3.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "pg": "^7.8.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "vue": "^2.6.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^1.1.4",
    "jest": "^24.1.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vue-jest": "^3.0.3",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.6.5",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "./src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}
